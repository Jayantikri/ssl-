### WORDPRESS SITE URL CHANGE
UPDATE wp_options SET option_value = replace(option_value, 'http://13.200.45.193/wordpress', 'http://wp.akashgaur.xyz') WHERE option_name = 'home' OR option_name = 'siteurl';

UPDATE wp_posts SET guid = replace(guid, 'http://13.200.45.193/wordpress','http://wp.akashgaur.xyz');

UPDATE wp_posts SET post_content = replace(post_content, 'http://13.200.45.193/wordpress', 'http://wp.akashgaur.xyz');

UPDATE wp_postmeta SET meta_value = replace(meta_value,'http://13.200.45.193/wordpress',  'http://wp.akashgaur.xyz');
